{
    "agent": "InAC",
    "problem": "MinariOffline",
    "total_steps": 100000,
    "episode_cutoff": -1,
    "metaParameters": {
        "experiment": {
            "dataset": "plant-rl/continuous-v8",
            "log_interval": 10000,
            "eval_interval": 10000,
            "seed_offset": 0,
            "state_dim": 7,
            "action_dim": 3,
            "discrete_control": false
        },
        "agent": {
            "policy_type": "dirichlet",
            "tau": 1e-3,
            "learning_rate": 1e-4,
            "hidden_units": 256,
            "batch_size": 256,
            "gamma": 0.99,
            "use_target_network": 1,
            "target_network_update_freq": 1,
            "polyak": 0.995,
            "weight_decay": 1e-4,
            "clip_grad_norm": 1.0
        }
    }
