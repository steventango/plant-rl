{
    "agent": "GreedyAC",
    "problem": "MultiPlantSimulator",
    "total_steps": 10000,
    "episode_cutoff": -1,
    "metaParameters": {
        "environment":{
            "num_plants": 32,
            "lag": 1
        },
        "replay_capacity": 10000,
        "batch_size": 32,
        "tau": 0.01,
		"beta1": 0.9,
        "beta2": 0.999,
        "policy_type": "Softmax",
        "target_update_interval": 1,
        "critic_lr": [3e-3, 4e-3, 5e-3, 6e-3, 7e-3, 8e-3],
        "actor_lr_scale": 0.1,
        "hidden_dim": 32,
        "n_hidden": 2,
        "weight_init": "xavier_uniform",
        "clip_stddev": 1000,
		"cuda": false,
        "batch_observations": true
    }
}
