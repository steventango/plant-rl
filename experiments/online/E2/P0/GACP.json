{
    "agent": "GreedyAC-Planning",
    "problem": "PlantGrowthChamberBinary",
    "total_steps": 30,
    "episode_cutoff": -1,
    "metaParameters": {
        "environment": {
            "camera_url": "http://mitacs-zone02-camera01.ccis.ualberta.ca:8000/observation",
            "lightbar_url": "http://mitacs-zone2.ccis.ualberta.ca:8000/action/left"
        },
        "experiment": {
            "step_duration": 60,
            "update_freq": 1
        },
        "replay_capacity": 100000,
        "batch_size": 4,
        "tau": 0.01,
		"beta1": 0.9,
        "beta2": 0.999,
        "policy_type": "Softmax",
        "target_update_interval": 1,
        "critic_lr": 1e-2,
        "actor_lr_scale": 10.0,
        "hidden_dim": 32,
        "n_hidden": 2,
        "weight_init": "xavier_uniform",
        "clip_stddev": 1000,
		"cuda": false,
        "use_planning": true
    }
}
